<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Brightcove Video Player"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[brightcove.player.dialogs]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <tab-styles
                jcr:primaryType="nt:unstructured"
                jcr:title="Advanced"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldset-container-embed-type
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <embedType jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <iframe
                                                jcr:primaryType="nt:unstructured"
                                                disabled="{Boolean}true"/>
                                        </items>
                                    </embedType>
                                </items>
                            </fieldset-container-embed-type>
                            <fieldset-container-attributes
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Component Attributes"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <containerID
                                        jcr:primaryType="nt:unstructured"
                                        fieldLabel="Anchor ID"/>
                                    <containerClass
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}true"/>
                                </items>
                            </fieldset-container-attributes>
                            <fieldset-player-styles jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <size
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}true"/>
                                </items>
                            </fieldset-player-styles>
                        </items>
                    </column>
                </items>
            </tab-styles>
        </items>
    </content>
</jcr:root>
