<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Topic Teaser"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.teaser.v2.editor,core.wcm.components.image.v3.editor,cq.author.pathfield]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <actions jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <link
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </actions>
                    <icons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icons"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            
                                            <icon-with-custom
                                                jcr:primaryType="nt:unstructured"
                                                label="Icon"
                                                sling:resourceType="acs-commons/granite/ui/components/include"
                                                path="/apps/kpmg/components/utilities/icon-with-custom"
                                                margin="{Boolean}false">
                                                <parameters
                                                    jcr:primaryType="nt:unstructured"
                                                    label="Icon"
                                                    uniqueId="icon"
                                                    iconTypeName="iconType"
                                                    materialIconValue="materialIcon"
                                                    materialIconName="icon"
                                                    customIconValue="customIcon"
                                                    customIconName="customIcon"
                                                />
                                            </icon-with-custom>
                                            <eyebrow-icon-with-custom
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Insights"
                                                sling:resourceType="acs-commons/granite/ui/components/include"
                                                path="/apps/kpmg/components/utilities/icon-with-custom"
                                                margin="{Boolean}false">
                                                <parameters
                                                    jcr:primaryType="nt:unstructured"
                                                    label="Eyebrow Icon"
                                                    uniqueId="eyebrow"
                                                    iconTypeName="eyebrowType"
                                                    materialIconValue="materialEyebrow"
                                                    materialIconName="eyebrow"
                                                    customIconValue="customEyebrow"
                                                    customIconName="customEyebrow"
                                                />
                                            </eyebrow-icon-with-custom>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </icons>
                    <text jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titleGroup jcr:primaryType="nt:unstructured">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <title
                                                        jcr:primaryType="nt:unstructured"
                                                        required="{Boolean}true"/>
                                                </items>
                                            </titleGroup>
                                            <descriptionGroup jcr:primaryType="nt:unstructured">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <description
                                                        jcr:primaryType="nt:unstructured"
                                                        required="{Boolean}true"/>
                                                </items>
                                            </descriptionGroup>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </text>
                     <image
                        jcr:primaryType="nt:unstructured"
                        sling:orderBefore="actions"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
