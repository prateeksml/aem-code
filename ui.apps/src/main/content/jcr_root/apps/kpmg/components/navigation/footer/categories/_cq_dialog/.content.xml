<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Footer Categories"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-image__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <extraClientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                js="kpmg-footer"/>
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Organizational Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <category1
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Category 1"
                                                sling:resourceType="acs-commons/granite/ui/components/include"
                                                path="/apps/kpmg/components/navigation/footer/categories/dialog-partial/category-block"
                                                margin="{Boolean}true">
                                                <parameters
                                                    jcr:primaryType="nt:unstructured"
                                                    categoryName="contact"
                                                    max="4"
                                                    min="1"
                                                    categoryLabel="Contact"
                                                    readOnlyTitle="{Boolean}true"/>
                                            </category1>
                                            <category2
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Category 2"
                                                sling:resourceType="acs-commons/granite/ui/components/include"
                                                path="/apps/kpmg/components/navigation/footer/categories/dialog-partial/category-block"
                                                margin="{Boolean}true">
                                                <parameters
                                                    jcr:primaryType="nt:unstructured"
                                                    categoryName="media"
                                                     max="4"
                                                    min="1"
                                                    categoryLabel="Media"
                                                    readOnlyTitle="{Boolean}true"/>
                                            </category2>
                                            <category3
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Category 3"
                                                sling:resourceType="acs-commons/granite/ui/components/include"
                                                path="/apps/kpmg/components/navigation/footer/categories/dialog-partial/category-block"
                                                margin="{Boolean}true">
                                                <parameters
                                                    jcr:primaryType="nt:unstructured"
                                                    categoryName="others"
                                                     max="4"
                                                    min="0"
                                                    categoryLabel=""
                                                    readOnlyTitle="{Boolean}false"/>
                                            </category3>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
