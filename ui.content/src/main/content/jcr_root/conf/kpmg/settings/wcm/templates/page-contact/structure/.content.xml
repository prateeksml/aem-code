<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/kpmg]"
        cq:template="/conf/kpmg/settings/wcm/templates/page-contact"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="kpmg/components/structure/page-contact">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kpmg/components/container/container"
            layout="simple">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kpmg/components/fragments/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/kpmgpublic/language-masters/en/site/header/master"/>
            <heropeople
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kpmg/components/content/hero-people"
                editable="{Boolean}true"/>
            <main
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kpmg/components/container/main"
                editable="{Boolean}true"/>
            <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kpmg/components/fragments/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/kpmgpublic/language-masters/en/site/footer/master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1024"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
